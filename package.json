{
  "name": "insta-post",
  "version": "0.1.0",
  "private": true,
  "description": "Gerador de Posts para Redes Sociais com IA",
  "scripts": {
    "dev": "concurrently \"npm:dev:frontend\" \"npm:dev:backend\"",
    "dev:frontend": "next dev",
    "dev:backend": "tsx watch src/api/server.ts",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "next build",
    "build:backend": "tsc --project tsconfig.api.json",
    "start": "concurrently \"npm:start:frontend\" \"npm:start:backend\"",
    "start:frontend": "next start",
    "start:backend": "node dist/api/server.js",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "dependencies": {
    "@prisma/client": "^5.9.1",
    "@supabase/supabase-js": "^2.39.0",
    "@trigger.dev/sdk": "^3.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "next": "^14.0.4",
    "next-intl": "^4.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stripe": "^14.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "dotenv": "^17.2.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "prisma": "^5.9.1",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
